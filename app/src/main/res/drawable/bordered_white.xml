<?xml version="1.0" encoding="utf-8"?>
<layer-list
	xmlns:android="http://schemas.android.com/apk/res/android">

	<item>
		<shape>
			<corners android:radius="5dp" />
			<solid android:color="@color/colorPrimary" />
		</shape>
	</item>
	<item
		android:bottom="3dp"
		android:left="3dp"
		android:right="3dp"
		android:top="3dp">
		<shape>
			<corners android:radius="5dp" />
			<solid android:color="@android:color/white" />
		</shape>
	</item>
</layer-list>